# Auto-discovery settings
discovery_settings:
  # Enable automatic documentation discovery
  auto_discover: true
  
  # Paths to scan for documentation references
  scan_paths:
    - "docs/"
    - "README.md"
    - "*.md"
    - ".agent-os/"
  
  # File patterns to include
  include_patterns:
    - "*.md"
    - "*.yaml"
    - "*.yml"
    - "*.json"
  
  # File patterns to exclude
  exclude_patterns:
    - "node_modules/"
    - ".git/"
    - ".venv/"
    - "dist/"
    - "build/"

# Documentation processing metadata
processing_metadata:
  total_sources: 0
  last_discovery: "YYYY-MM-DD"
  auto_refresh_enabled: true
  cache_size_mb: 0
  
