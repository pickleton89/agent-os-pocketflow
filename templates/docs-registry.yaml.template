# Registry metadata
metadata:
  created_by: "Agent OS Framework"
  framework_version: "1.4.0"
  registry_format_version: "1.0.0"
  
  # Tracking for updates and maintenance
  maintenance:
    check_frequency: "weekly"
    auto_update: false
    notification_threshold: 30  # days before staleness warning